@model HomeViewModel

<div>
    @await Html.PartialAsync("Carousel")
</div>

<div class="container mt-4">
    <h2 class="m-2">These are our best sellers: </h2>
    <div class="row mt-4 mb-2">
        @{ foreach(Car car in Model.favCars) {
                @await Html.PartialAsync("AllCars", car) } }
    </div>
</div>